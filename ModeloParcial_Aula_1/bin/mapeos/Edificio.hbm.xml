<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Edificio" table="edificio">
		<id name="idEdificio" column="idEdificio">
			<generator class="identity"></generator>
		</id>
		<property name="edificio" column="edificio" type="string"></property>
		<set name="aulas" table="aula" cascade="save-update" lazy="true" fetch="select" order-by="idAula asc">
			<key column="idEdificio"></key>
			<one-to-many class="datos.Aula"/>
		</set>
	</class>
</hibernate-mapping>